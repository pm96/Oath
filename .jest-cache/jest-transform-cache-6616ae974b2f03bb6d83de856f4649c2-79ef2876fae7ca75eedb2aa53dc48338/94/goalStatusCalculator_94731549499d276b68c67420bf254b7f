function isSameDay(date1, date2) {
  return date1.getFullYear() === date2.getFullYear() && date1.getMonth() === date2.getMonth() && date1.getDate() === date2.getDate();
}
function getDeadlineProximity(deadline) {
  var now = new Date();
  var hoursUntilDeadline = (deadline.getTime() - now.getTime()) / (1000 * 60 * 60);
  var isOverdue = hoursUntilDeadline < 0;
  var displayText;
  if (isOverdue) {
    var hoursOverdue = Math.abs(hoursUntilDeadline);
    var daysOverdue = Math.floor(hoursOverdue / 24);
    if (daysOverdue > 0) {
      displayText = `Overdue by ${daysOverdue}d`;
    } else {
      displayText = `Overdue by ${Math.floor(hoursOverdue)}h`;
    }
  } else {
    var minutesUntilDeadline = Math.floor(hoursUntilDeadline * 60);
    if (hoursUntilDeadline < 1) {
      displayText = `Due in ${minutesUntilDeadline}m`;
    } else if (hoursUntilDeadline < 24) {
      displayText = `Due in ${Math.floor(hoursUntilDeadline)}h`;
    } else {
      var daysUntilDeadline = Math.floor(hoursUntilDeadline / 24);
      displayText = `Due in ${daysUntilDeadline}d`;
    }
  }
  return {
    hoursUntilDeadline: hoursUntilDeadline,
    isOverdue: isOverdue,
    displayText: displayText
  };
}
function calculateStatus(goal) {
  var now = new Date();
  var deadline = goal.nextDeadline;
  var hoursUntilDeadline = (deadline.getTime() - now.getTime()) / (1000 * 60 * 60);
  if (goal.latestCompletionDate && isSameDay(goal.latestCompletionDate, now)) {
    return {
      color: "green",
      priority: 3,
      text: "Completed âœ“",
      showNudge: false
    };
  }
  if (hoursUntilDeadline < 0) {
    var hoursOverdue = Math.abs(hoursUntilDeadline);
    var daysOverdue = Math.floor(hoursOverdue / 24);
    return {
      color: "red",
      priority: 1,
      text: daysOverdue > 0 ? `Overdue by ${daysOverdue}d` : `Overdue by ${Math.floor(hoursOverdue)}h`,
      showNudge: true
    };
  }
  if (hoursUntilDeadline < 2) {
    var minutesUntilDeadline = Math.floor(hoursUntilDeadline * 60);
    return {
      color: "red",
      priority: 1,
      text: `Due in ${minutesUntilDeadline}m`,
      showNudge: true
    };
  }
  if (hoursUntilDeadline < 6) {
    return {
      color: "yellow",
      priority: 2,
      text: `Due in ${Math.floor(hoursUntilDeadline)}h`,
      showNudge: true
    };
  }
  return {
    color: "green",
    priority: 3,
    text: "Safe",
    showNudge: false
  };
}
function getStatusColor(status) {
  switch (status.color) {
    case "green":
      return "#22c55e";
    case "yellow":
      return "#eab308";
    case "red":
      return "#ef4444";
    default:
      return "#6b7280";
  }
}
function getStatusText(goal) {
  var status = calculateStatus(goal);
  return status.text;
}
function shouldShowNudgeButton(goal) {
  var isCompleted = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : false;
  if (isCompleted) {
    return false;
  }
  var now = new Date();
  if (goal.latestCompletionDate && isSameDay(goal.latestCompletionDate, now)) {
    return false;
  }
  var status = calculateStatus(goal);
  return status.showNudge;
}
