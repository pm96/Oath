function isValidAnimationDuration(duration) {
  return duration > 0 && duration <= 2000;
}
function createAnimationTiming(duration, easing) {
  if (!isValidAnimationDuration(duration)) {
    throw new Error(`Invalid animation duration: ${duration}ms. Must be between 1-2000ms.`);
  }
  return {
    duration: duration,
    easing: easing || animationConfig.easing.easeInOut
  };
}
